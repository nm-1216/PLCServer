[
  {
    /*WCS-Crane1_PLC交互信号*/
    "Name": "Crane1",
    "IpAddress": "192.168.0.30",
    "Port": 44818,
    /*西门子型号
    S1200	1	1200系列
    S300	2	300系列
    S400	3	400系列
    S1500	4	1500系列PLC
    S200Smart	5	200的smart系列
    S200	6	200系统，需要额外配置以太网模块
    */
    "Siemens": 1,
    /*AB 需要用*/
    "Slot": 0,

    "ProviderName": "SiemensS7Net",
    "List": [
      /*WCS->PLC*/
      {"name": "C1_SRMNO","Address": "DB510.0","type": "int16"},//堆垛机号
      {"name": "C1_TaskNO","Address": "DB510.2","type": "int16"},//任务号
      {"name": "C1_StrAddRow","Address": "DB510.4","type": "int16"},//起始作业地址-排
      {"name": "C1_StrAddCol","Address": "DB510.6","type": "int16"},//起始作业地址-列
      {"name": "C1_StrAddLay","Address": "DB510.8","type": "int16"},//起始作业地址-层
      {"name": "C1_EndAddRow","Address": "DB510.10","type": "int16"},//终点作业地址-排
      {"name": "C1_EndAddCol","Address": "DB510.12","type": "int16"},//终点作业地址-列
      {"name": "C1_EndAddLay","Address": "DB510.14","type": "int16"},//终点作业地址-层
      {"name": "C1_SRMAction","Address": "DB510.16","type": "int16"},//作业命令 0=默认；1=作业开始，2=作业完成确认
      {"name": "C1_StrExeSkid","Address": "DB510.22","type": "int16"},//堆垛机开始执行的任务滑撬号(作业开始前写入，任务结束后清零)
      {"name": "C1_CheckSkid","Address": "DB510.26","type": "int16"},//堆垛机滑撬号验证反馈 0=初始值；1=验证成功；2=验证失败
      {"name": "C1_TaskError","Address": "DB510.28","type": "int16"},//WCS写入异常信息  0=无异常；1=任务发送失败
      {"name": "C1_LockState","Address": "DB510.90","type": "int16"},//库台锁定状态 0=初始值；1=入库锁定；2=出库锁定
      /*WCS<-PLC*/
      {"name": "C1_SRMNumber","Address": "DB511.0", "type": "int16"},//堆垛机号
      {"name": "C1_SRMWorkMode","Address": "DB511.2", "type": "int16"},//工作模式  0=不明；1=手动；2=半自动；3=自动；
      {"name": "C1_SRMWorkState","Address": "DB511.4", "type": "int16"},//作业状态 0=不明；1=待机；10=取货中；11=取货完成；12=请求WCS验证当前任务中的滑撬号；20=放货中；21=放货完成；
      {"name": "C1_SRMError","Address": "DB511.6", "type": "int16"},//故障报警 0=堆垛机正常；1=堆垛机处于故障状态
      {"name": "C1_SRMReadSkid","Address": "DB511.10","type": "int16"},//堆垛机请求WCS验证时读取的滑撬号 （取货完成并读取成功后写入，任务结束后清零）
      
      /* 7m层彩车身库台（仅入库) */
      {"name": "C1_7MPStageState","Address": "DB511.50","type": "int16"},//库台可用状态 0=不明；1=可入；2=可出；3=可入/可出；4=满载；-1=禁用
      {"name": "C1_7MPStageError","Address": "DB511.52","type": "int16"},//库台设备故障 0-库台设备正常；1=库台设备处于故障状态
      {"name": "C1_7MPStageInPlace","Address": "DB511.54","type": "int16"},//库台入库滑撬到位  0=滑撬未到位；1=滑撬到位
      {"name": "C1_7MPStageSkid","Address": "DB511.56","type": "int16"},//库台入库点滑撬号  用于入库点验证
      /* 7m层彩车身库台（仅入库) */

      /* 4m层彩车身库台（仅出库) */
      {"name": "C1_4MPStageState","Address": "DB511.70","type": "int16"},//库台可用状态 0=不明；1=可入；2=可出；3=可入/可出；4=满载；-1=禁用
      {"name": "C1_4MPStageError","Address": "DB511.72","type": "int16"}, //库台设备故障 0-库台设备正常；1=库台设备处于故障状态
      /* 4m层彩车身库台（仅出库) */

      /* 7m层白车身库台（可入/可出) */
      {"name": "C1_7MWStageState","Address": "DB511.90","type": "int16"},//库台可用状态 0=不明；1=可入；2=可出；3=可入/可出；4=满载；-1=禁用
      {"name": "C1_7MWStageError","Address": "DB511.92","type": "int16"},//库台设备故障 0-库台设备正常；1=库台设备处于故障状态
      {"name": "C1_7MWStageInPlace","Address": "DB511.94","type": "int16"},//库台入库滑撬到位 0=滑撬未到位；1=滑撬到位
      {"name": "C1_7MWStageSkid","Address": "DB511.96","type": "int16"},//库台入库点滑撬号 用于入库点验证
      /* 7m层白车身库台（可入/可出) */
            
      /* 4m层空滑撬库台（仅入库）*/
      {"name": "C1_4MEStageState","Address": "DB511.110","type": "int16"},//库台可用状态 0=不明；1=可入；2=可出；3=可入/可出；4=满载；-1=禁用
      {"name": "C1_4MEStageError","Address": "DB511.112","type": "int16"},//库台设备故障 0-库台设备正常；1=库台设备处于故障状态
      {"name": "C1_4MEStageInPlace","Address": "DB511.114","type": "int16"},//库台入库滑撬到位 0=滑撬未到位；1=滑撬到位
      {"name": "C1_4MEStageSkid","Address": "DB511.116","type": "int16"} //库台入库点滑撬号 用于入库点验证
      /* 4m层空滑撬库台（仅入库） */
    ]
  },

